# Local development defaults (outside Docker)
# Create a local-only override (.env.local) for real secrets
DATABASE_URL=postgresql://fourmore:fourmore_dev_password@localhost:5432/fourmore

# OSM OAuth (get from https://www.openstreetmap.org/oauth2/applications)
OSM_CLIENT_ID=your_osm_client_id
OSM_CLIENT_SECRET=your_osm_client_secret
OSM_REDIRECT_URI=http://127.0.0.1:3000/auth/callback

# JWT
JWT_SECRET_KEY=your_super_secret_jwt_key_here
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60

# Redis
REDIS_URL=redis://localhost:6379

# Frontend API prefix
VITE_API_BASE_URL=http://127.0.0.1:8000
